{
    "name": "azuriom-plugin/dune-rp",
    "description": "Dune Awakening RP Plugin for Azuriom - Complete roleplay system with Houses, Characters, Spice economy and Events",
    "type": "azuriom-plugin",
    "version": "1.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "nke69",
            "email": "contact@nke69.dev",
            "homepage": "https://github.com/nke69"
        }
    ],
    "keywords": [
        "azuriom",
        "plugin",
        "dune",
        "roleplay",
        "rp",
        "gaming"
    ],
    "homepage": "https://github.com/nke69/azuriom-dune-rp",
    "support": {
        "issues": "https://github.com/nke69/azuriom-dune-rp/issues",
        "source": "https://github.com/nke69/azuriom-dune-rp"
    },
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "azuriom/framework": "^1.2",
        "laravel/framework": "^9.0|^10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Azuriom\\Plugin\\DuneRp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Azuriom\\Plugin\\DuneRp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Azuriom\\Plugin\\DuneRp\\Providers\\DuneRpServiceProvider",
                "Azuriom\\Plugin\\DuneRp\\Providers\\RouteServiceProvider"
            ]
        },
        "azuriom": {
            "id": "dune-rp",
            "name": "Dune Awakening RP",
            "api-version": "1.2.0"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
